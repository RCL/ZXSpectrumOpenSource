{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "dezog",
			"request": "launch",
			"name": "CSpect",
			"remoteType": "cspect",
			"sjasmplus": [
				{
					"path": "telegrongy-clean.sld.txt"
				}
			],
			"commandsAfterLaunch": [],
			"history": {
				"reverseDebugInstructionCount": 1000000,
				"spotCount": 10,
				"codeCoverageEnabled": false
			},
			"startAutomatically": false,
			"rootFolder": "${workspaceFolder}",
			"load": "telegrongy.sna",
			"topOfStack": "main"
		},
		{
            "type": "dezog",
            "request": "launch",
            "name": "CSpect_RE",
            "remoteType": "cspect",
            "startAutomatically": false,
            "revEng": [
                {
                    "path": "dezog_re.list"
                }
            ],
            "rootFolder": "${workspaceFolder}"
        }
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				//"--disable-extensions",
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"debugWebviews": true,
			"sourceMaps": true,
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "watch",
			"skipFiles": [
				"**/extensionHostProcess.js"
			]
		}
	]
}